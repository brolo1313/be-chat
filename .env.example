# Environment (PROD or DEV)
ENV=DEV

# JWT Token expiration time
EXPIRES_IN=1h

# MongoDB Connection String
# For PROD: use MongoDB Atlas connection string
# For DEV: use mongodb://mongodb:27017/chat (local Docker MongoDB)
MONGO_URL='mongodb://mongodb:27017/chat'

# Secret key for JWT
SECRET_KEY='your-secret-key-here'
